{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "ignoreExportsUsedInFile": {
    "interface": true,
    "type": true
  },
  "tags": ["-lintignore"],
  "ignoreDependencies": ["lint-staged", "tailwindcss", "tw-animate-css"],
  "ignoreBinaries": ["husky"],
  "entry": ["src/app/**/*.{ts,tsx}", "!src/app/**/*.test.{ts,tsx}"],
  "project": ["src/**/*.{ts,tsx}"],
  "ignore": ["src/components/ui/**", "src/components/index.ts", "src/auth.ts"]
}
